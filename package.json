    "test": "nyc --check-coverage --reporter=html --reporter=text mocha --timeout 60000",
    "coverage": "npx nyc report --reporter=text-summary",
    "check": "concurrently \"npm run lint\" \"npm run inspect\"",
    "start": "cd backend && npm start"
