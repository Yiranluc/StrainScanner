    "frontend": "cd frontend && npm run serve",
    "dev": "npm run check && concurrently \"cd backend && npm run dev\" \"npm run frontend\"",
    "postinstall": "concurrently \"cd backend && npm install\" \"cd frontend && npm install\"",
